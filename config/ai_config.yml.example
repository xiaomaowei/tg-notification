# AI服务配置
ai_services:
  # ChatGPT-官方配置
  chatgpt-official:
    provider: openai
    api_key: your_openai_api_key
    api_url: https://api.openai.com/v1/chat/completions
    model: gpt-4o
    temperature: 0.8
    max_tokens: 4096

  # ChatGPT配置
  chatgpt-gptnb:
    provider: openai
    api_key: your_gptnb_api_key
    api_url: https://goapi.gptnb.ai/v1/chat/completions
    model: gpt-4o-mini
    temperature: 0.8
    max_tokens: 4096

  # DeepSeek配置
  deepseek-gptnb:
    provider: deepseek
    api_key: your_gptnb_api_key
    api_url: https://goapi.gptnb.ai/v1/chat/completions
    model: deepseek-chat
    temperature: 0.8
    max_tokens: 4096

# 默认使用的AI服务
default_service: chatgpt-official 